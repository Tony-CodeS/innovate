variable "project_name" {
  description = "Name prefix for all resources"
  type        = string
  default     = "innovatemart"
}

variable "region" {
  description = "AWS region"
  type        = string
  default     = "eu-west-1"
}

variable "vpc_cidr" {
  description = "VPC CIDR block"
  type        = string
  default     = "10.0.0.0/16"
}
